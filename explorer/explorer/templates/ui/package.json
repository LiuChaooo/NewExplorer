    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-uglify": "^3.3.0",
    "grunt-contrib-watch": "*",
    "grunt-css": "~0.5.4",
    "grunt-macreload": "*",
    "grunt-markdown": "^0.7.0",
    "grunt-pofriend": "^1.0.1",
    "grunt-replace": "^1.0.1"
